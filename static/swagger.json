    "info": {
        "title": "FarmBot API",
        "version": "3.0",
        "description": "Endpoints for the Smart Greenhouse System by ETCE-LAB"
    },
    "produces": [
        "application/json"
    ],
    "consumes": [
        "application/json"
    ],
    "tags": [
        {
            "name": "station",
            "description": "Endpoints for the Weather Station"
        },
        {
            "name": "forecast",
            "description": "Endpoints for Weather Forecast"
        },
        {
