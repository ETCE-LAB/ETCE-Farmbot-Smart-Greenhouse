        }
    ],
    "definitions": {
        "WeatherStationData": {
            "required": [
                "measurement_type",
                "measurement_value",
                "received_at"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "description": "The unique identifier of the measurement"
                },
                "measurement_value": {
                    "type": "number",
                    "description": "Value of the measurement"
                },
                "measurement_type": {
                    "type": "string",
                    "description": "Type of the measurement"
                },
                "received_at": {
                    "type": "string",
                    "description": "ISO date string when the measurement was received"
                }
            },
            "type": "object"
        },
        "WeatherForecastData": {
            "required": [
                "date"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "description": "The unique identifier of the forecast"
                },
                "date": {
                    "type": "string",
                    "description": "The date of the forecast"
                },
                "max_temperature": {
                    "type": "number",
                    "description": "The maximum temperature of the day"
                },
                "min_temperature": {
                    "type": "number",
                    "description": "The minimum temperature of the day"
                },
                "sunshine_duration_minutes": {
                    "type": "integer",
                    "description": "The duration of sunshine in minutes"
                },
                "precipitation_mm": {
                    "type": "number",
                    "description": "The amount of precipitation over the day in mm. 1mm = 1l/m\u00b2"
                }
            },
            "type": "object"
        },
        "WaterManagementData": {
            "required": [
                "date"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "description": "The unique identifier of the measurement"
                },
                "date": {
                    "type": "string",
                    "description": "The date of the reading"
                },
                "volume": {
                    "type": "number",
                    "description": "The stored volume of water in liters"
                }
            },
            "type": "object"
        }
    },
    "responses": {
        "ParseError": {
            "description": "When a mask can't be parsed"
        },
        "MaskError": {
            "description": "When any error occurs on mask"
        }
    }
